{
  "name": "markdown_editor",
  "version": "1.0.0",
  "description": "",
  "main": "dist/js/main/index.js",
  "scripts": {
    "clean": "rimraf .tmp dist release",
    "build": "webpack",
    "watch": "webpack -w",
    "start": "electron .",
    "eslint": "eslint \"src/**/*.{js,jsx}\" test",
    "lint": "npm run eslint; stylelint src/**/*.css",
    "test": "NODE_ENV=test mocha test/**/*_test.js",
    "copy:resources": "cpx \"prod_resources/**/*\" dist",
    "copy:photon": "cpx \"node_modules/photon/dist/css/**/*\" dist/css && cpx \"node_modules/photon/dist/fonts/**/*\" dist/fonts",
    "prebundle": "npm run copy:resources && npm run copy:photon",
    "bundle": "webpack",
    "package": "npm run bundle && electron-packager dist \"Electron Markdown\" --all --out=release --icon=\"icon/icon\""
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "babel-loader": "^6.2.10",
    "emojione": "2.2.7",
    "marked": "0.3.7",
    "photon": "github:connors/photon",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "webpack": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "6.18.0",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "cpx": "^1.3.1",
    "css-loader": "0.23.1",
    "electron": "1.6.1",
    "electron-builder": "^14.4.0",
    "electron-builder-squirrel-windows": "^13.10.1",
    "electron-packager": "^8.5.2",
    "eslint": "3.14.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "6.9.0",
    "jsdom": "9.6.0",
    "mocha": "3.1.0",
    "spectron": "3.6.0",
    "style-loader": "0.13.1",
    "stylelint": "7.7.1",
    "stylelint-config-standard": "15.0.1",
    "webpack": "2.2.0"
  }
}
